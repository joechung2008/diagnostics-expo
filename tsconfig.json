{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"]
    },
    "strict": true,
    "types": ["jest"]
  },
  "exclude": ["dist/*", "react/*"],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "eslint.config.js",
    "lib/**/*.ts"
  ]
}
